<div class="container">
  <div id="question_form">
    <%= form_for @question do |f| %>

      <%= f.label :title, class: 'question-labels' %><br>
      <%= f.text_area :title %><br>

      <%= f.label :prompt, class: 'question-labels' %><br>
      <%= f.text_area :prompt %><br>

      <%= f.submit 'Create Question', :class => 'create_question_submit' %>
    <% end %>
  </div>
</div>
